{
  "manifest_version": 2,
  "name": "Chrome Extension TypeScript Starter",
  "description": "Chrome Extension, TypeScript, React",
  "version": "1.0",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "build/common_for_scripts.js",
      "build/background_script.js"
    ]
  },
 
 "content_security_policy": "script-src  https://apis.google.comâ€‹",

  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ]
}
